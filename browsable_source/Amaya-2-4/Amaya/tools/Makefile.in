@VPATHOPT@= @srcdir@

include ../Options

SUBDIRS = mkdep dns_daemon javastub cextract-1.7

@SET_MAKE@

all : 
	@(for dir in $(SUBDIRS) ;\
	  do if test -d $$dir ; then cd $$dir ; \
	  $(MAKE) CC="$(CC)" CFLAGS="$(CFLAGS)" all ; \
	  cd .. ; fi ;done )

clean :
	@(for dir in $(SUBDIRS) ;\
	  do if test -d $$dir ; then cd $$dir ; \
	  $(MAKE) CC="$(CC)" CFLAGS="$(CFLAGS)" clean ; \
	  cd .. ; fi ;done )

install :
	@(for dir in $(SUBDIRS) ;\
	  do if test -d $$dir ; then cd $$dir ; \
	  $(MAKE) CC="$(CC)" CFLAGS="$(CFLAGS)" install ; \
	  cd .. ; fi ;done )

uninstall :
	@(for dir in $(SUBDIRS) ;\
	  do if test -d $$dir ; then cd $$dir ; \
	  $(MAKE) CC="$(CC)" CFLAGS="$(CFLAGS)" uninstall ; \
	  cd .. ; fi ;done )

depend : 
	@(for dir in $(SUBDIRS) ;\
	  do if test -d $$dir ; then cd $$dir ; \
	  $(MAKE) CC="$(CC)" CFLAGS="$(CFLAGS)" depend ; \
	  cd .. ; fi ;done )

