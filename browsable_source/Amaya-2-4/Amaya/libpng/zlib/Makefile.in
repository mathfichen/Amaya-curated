@VPATHOPT@= @srcdir@
THOTDIR = @top_srcdir@

include ../../Options

INCLUDES= -I../.. -I@srcdir@

prefix = @prefix@
exec_prefix = @exec_prefix@
libdir = @libdir@

#
# Rule to build objects
#
.c.o :
	$(CC) $(CFLAGS) $(INCLUDES) -c $< -o $@

all : ../../libz.a

clean :
	$(RM) libz.a
	$(RM) *.o

install :

uninstall :

include .depends

../../tools/mkdep/mkdep : 
	@(cd ../../tools/mkdep ; $(MAKE) CC="$(CC)" CFLAGS="$(CFLAGS)")

.depends depend : ../../tools/mkdep/mkdep
	../../tools/mkdep/mkdep -relative -vpath $(VPATH) $(INCLUDES) $(VPATH)/*.c  > .depends

#########################################################################
#									#
#		Rules to build the library				#
#									#
#########################################################################

Z_OBJ = \
	adler32.o \
	compress.o \
	crc32.o \
	deflate.o \
	gzio.o \
	infblock.o \
	infcodes.o \
	inffast.o \
	inflate.o \
	inftrees.o \
	infutil.o \
	trees.o \
	uncompr.o \
	zutil.o

../../libz.a : $(Z_OBJ)
	$(AR) ../../libz.a $(Z_OBJ)
	$(RANLIB) ../../libz.a


