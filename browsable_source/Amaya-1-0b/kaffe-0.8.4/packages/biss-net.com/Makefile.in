srcdir=		@srcdir@
subdirs=	APIawt

all:
		@for i in $(subdirs); do (cd $$i ; $(MAKE) all) || exit 1 ; done

all-classes:

clean:
		for i in $(subdirs); do (cd $$i ; $(MAKE) clean) ; done

distclean:
		for i in $(subdirs); do (cd $$i ; $(MAKE) distclean) ; done
		rm -f Makefile
		-rmdir $(subdirs)

install:
		for i in $(subdirs); do (cd $$i ; $(MAKE) install) || exit 1 ; done
